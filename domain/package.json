{
  "name": "@windy-civi/domain",
  "version": "0.1.0",
  "scripts": {
    "test": "ts-node ./scripts/runTests.test.ts",
    "typecheck": "tsc",
    "prettier": "prettier '**/*.{js,ts,jsx,tsx,json,css,md}'",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "lint": "eslint '**/*.ts' --ignore-pattern node_modules/",
    "spellcheck": "cspell '**/*.{js,ts,jsx,tsx,json,css,md}'",
    "quality": "run-p lint typecheck format:check spellcheck"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cspell": "^8.14.2",
    "eslint": "^8.34.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "date-fns": "^3.3.1"
  }
}
